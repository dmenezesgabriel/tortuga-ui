{
  "name": "pareto-vue",
  "type": "module",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/pareto-ui.umd.cjs",
  "module": "./dist/pareto-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/pareto-ui.es.js",
      "require": "./dist/pareto-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "run-p type-check build-only",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "story-dev": "histoire dev",
    "story-build": "histoire build",
    "story-preview": "histoire preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "chart.js": "^3.8.0",
    "chartjs-adapter-date-fns": "^2.0.0",
    "chartjs-plugin-datalabels": "^2.1.0",
    "date-fns": "^2.29.2",
    "sass": "^1.53.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/types": "^7.19.3",
    "@histoire/plugin-vue": "^0.10.7",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/bootstrap": "^5.2.4",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.47",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "histoire": "^0.10.7",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^3.1.4",
    "vitest": "^0.15.1",
    "vue-tsc": "^0.38.1"
  }
}
